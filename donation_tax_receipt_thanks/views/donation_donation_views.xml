<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="donation_donation_view_form" model="ir.ui.view">
        <field name="name">donation.donation.view.form.inherit</field>
        <field name="model">donation.donation</field>
        <field name="inherit_id" ref="donation.donation_form" />
        <field
            name="groups_id"
            eval="[(4, ref('donation.group_donation_manager'))]"
        />
        <field name="priority" eval="20" />
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="lang" />
            </field>
        </field>
    </record>
</odoo>
