<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record model="ir.ui.view" id="view_base_module_update">
            <field name="name">base.module.update migration info</field>
            <field name="model">base.module.update</field>
            <field name="inherit_id" ref="base.view_base_module_update" />
            <field name="priority" eval="20" />
            <field name="arch" type="xml">
                <xpath expr="//div[@states='init']" position="inside">
                    <button
                        name="button_get_migration_info"
                        type="object"
                        string="Get Migration Info"
                    />
                </xpath>
            </field>
        </record>

    </data>
</odoo>
