<odoo>
    <record id="hr_contract_form" model="ir.ui.view">
        <field name="name">hr.contract.form Norway</field>
        <field name="model">hr.contract</field>
        <field name="inherit_id" ref="hr_contract.hr_contract_view_form" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="norway" string="Norway">
                    <group name="norway" string="Norway">
                        <field name="l10n_no_antallTimerPerUkeSomEnFullStillingTilsvarer" />
                        <field name="l10n_no_Arbeidsforholdtype" />
                        <field name="l10n_no_Arbeidstidsordning" />
                        <field name="l10n_no_FormForAnsettelse" />
                        <field name="l10n_no_loennsansiennitet" />
                        <field name="l10n_no_sisteLoennsendringsdato" />
                        <field name="l10n_no_stillingsprosent" />
                        <field name="l10n_no_AarsakTilSluttdato" />
                    </group>
                    <button
                        name="l10n_no_action_new_leave"
                        type="object"
                        string="Create new leave"
                    />
                    <field name="leave_ids">
                        <tree create="0">
                            <field name="date_from" />
                            <field name="date_to" />
                            <field name="employee_id" />
                            <field name="state" invisible="1" />
                        </tree>
                    </field>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
