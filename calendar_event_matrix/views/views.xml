<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_resource_booking_form" model="ir.ui.view">
        <field name="name">resource.booking matrix</field>
        <field name="model">resource.booking</field>
        <field name="inherit_id" ref="resource_booking.resource_booking_form"/>
        <field name="type">form</field>
        <field name="priority" eval="200"/>
        <field name="arch" type="xml">
            <data>
                <field name="combination_id" position="attributes">
                    <attribute name="attrs">{'required': 0}</attribute>
                </field>
                <xpath expr="//field[@name='start']" position="before">
                    <field name="start_date" readonly="1"/>
                </xpath>
            </data>
        </field>
    </record>

</odoo>